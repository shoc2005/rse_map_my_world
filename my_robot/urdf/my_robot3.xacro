<?xml version='1.0'?>

<robot name="my_robot3" xmlns:xacro="http://www.ros.org/wiki/xacro">


<gazebo reference="left_wheel">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="right_wheel">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="chassis">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="tower_chassis">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="chassis_rear_lane">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="chassis_right_lane">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="chassis_left_lane">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="tower">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="camera_link_optical">
 
</gazebo>

  <xacro:include filename="$(find my_robot)/urdf/my_robot3.gazebo" />

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <joint name="tower_joint" type="fixed">
    <origin xyz="0 0 .125" rpy="0 0 0" />
    <parent link="tower_chassis"/>
    <child link="tower" />
  </joint>

  <joint name="tower_chassis_joint" type="fixed">
    <origin xyz=".25 0 -.05" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="tower_chassis" />
  </joint>

  <joint name="chassis_left_lane_joint" type="fixed">
    <origin xyz="-.15 .175 0" rpy="0 0 0"/>
    <axis xyz="1 1 1"/>
    <parent link="tower_chassis"/>
    <child link="chassis_left_lane"/>
  </joint>

  <joint name="chassis_right_lane_joint" type="fixed">
    <origin xyz="-.15 -.175 0" rpy="0 0 0"/>
    <axis xyz="1 1 1"/>
    <parent link="tower_chassis"/>
    <child link="chassis_right_lane"/>
  </joint>

  <joint name="chassis_rear_lane_joint" type="fixed">
    <origin xyz="-.225 0 -.05" rpy="0 0 0"/>
    <axis xyz="1 1 1"/>
    <parent link="chassis"/>
    <child link="chassis_rear_lane"/>
  </joint>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.125 -.05" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.125 -.05" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <parent link="camera"/>
    <child link="camera_link_optical"/>
  </joint>


  <joint name="camera_joint" type="fixed">
    <origin xyz=".08 0 .06" rpy="0 0 0"/>
    <parent link="tower_chassis"/>
    
    <child link="camera"/>
  </joint>


  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="tower"/>
    <child link="hokuyo"/>
  </joint>

  <link name='chassis'>
    <pose>0 0 0.05 0 0 0</pose>

    <inertial>
      <mass value="15"/>
      <origin xyz="-.005 0 -0.027" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='chassis_collision'>
      <mass value="15"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".3 .2 .2"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <mass value="15"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".3 .2 .2"/>
      </geometry>
      
    </visual>
  </link>

  <link name='chassis_left_lane'>
    <pose>0 0 0.05 0 0 0</pose>

    <inertial>
      <mass value="0.5"/>
      <origin xyz="0.0 0 -0.5" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='chassis_left_lane_collision'>
	<mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .05 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_left_lane_visual'>
	<mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .05 .1"/>
      </geometry>
      
    </visual>
  </link>

  <link name='chassis_right_lane'>
    <pose>0 0 0.05 0 0 0</pose>

    <inertial>
      <mass value="0.5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='chassis_right_lane_collision'>
	<mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .05 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_right_lane_visual'>
	<mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .05 .1"/>
      </geometry>
      
    </visual>
  </link>

  <link name='chassis_rear_lane'>
    <pose>0 0 0.05 0 0 0</pose>

    <inertial>
      <mass value="0.5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='chassis_rear_lane_collision'>
	 <mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".15 .4 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_rear_lane_visual'>
 	<mass value="0.5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".15 .4 .1"/>
      </geometry>
      
    </visual>

    <collision name='rear_caster_collision'>
	<mass value="0"/>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </collision>

    <visual name='rear_caster_visual'>
	<mass value="0"/>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

  </link>

  <link name='left_wheel'>
    <visual name='left_wheel_visual'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </visual>

    <collision name='left_wheel_collision'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </collision>

    <inertial name='left_wheel_inertial'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </inertial>
  </link>

  <link name='right_wheel'>
    <visual name='right_wheel_visual'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </visual>

    <collision name='right_wheel_collision'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </collision>

    <inertial name='right_wheel_inertial'>
	<origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
	<geometry>
		<cylinder radius="0.1" length="0.048"/>
	</geometry>
	<mass value="1.0"/>
	<inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
        />
     </inertial>
  </link>

  <link name='tower'>
    <visual name='tower_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
      
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </visual>

    <collision name='tower_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </collision>

    <inertial name='tower_inertial'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </inertial>
  </link>

<link name='tower_chassis'>
    <visual name='tower_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size=".2 .3 .1"/>
      </geometry>
      
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </visual>

    <collision name='tower_chassis_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size=".2 .3 .1"/>
      </geometry>
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </collision>

    <collision name='front_caster_collision'>
	<mass value="0"/>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
	<mass value="0"/>
      <origin xyz="0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

    <inertial name='tower_chassis_inertial'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size=".2 .3 .1"/>
      </geometry>
      <mass value="0.2"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
            />
    </inertial>
  </link>
  
  <link name="camera_link_optical">
  </link>
  
   
  <link name="camera">
    <visual name="camera_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/kinect.dae"/>
        
      </geometry>
      
      <mass value="0.1"/>
      <interia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </visual>

    <interial name="camera_interia">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <interia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </interial>
  </link>
  

  <link name="hokuyo">
    <visual name="hokuyo_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>

      <mass value="1e-5"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </visual>

    <collision name="hokuyo_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <mass value="1e-5"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </collision>

    <inertial name="hokuyo_inertial">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <mass value="1e-5"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>


</robot>
